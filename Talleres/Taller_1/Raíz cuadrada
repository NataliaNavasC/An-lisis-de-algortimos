# -- Declaración de variables
# Estos son un ejemplo de datos

n <- 7
E <- 0.00002
x <- 3

# -- Declaración de funiones
y <- function(n,x)
{
  (1/2)*(x+(n/x))
}

# -- Cálculo de una raíz por bisección
u <- y(n,x)

while( x-u > E)
{
 x <- y(n,x)
 u <- y(n,x)
}

print(u)
